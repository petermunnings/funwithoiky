<script src="@oikonomos.web.Helpers.JavascriptHelper.Content("~/Scripts/Site/Reports.js")" type="text/javascript"></script>
<script src="@oikonomos.web.Helpers.JavascriptHelper.Content("~/Scripts/Site/ReportsMap.js")" type="text/javascript"></script>
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&region=ZA"></script>

@{
    ViewBag.Title = "Reports : Map";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div style="width: 100%" >
    <div id="menu_reports" class="ui-widget" style="margin: 0 auto; text-align:center; width:800px">
        <div id="buttons_reports" style="text-align: left; height:40px" >
            <button id="button_churchList">@if (ViewBag.DislayWholeChurch)
                                           {
                                               <text>Church List</text>
                                           }
                                           else
                                           {
                                               <text>Group List</text>
                                           }
                                           </button>
            <button id="button_churchMap">Map</button>
        </div>
    </div>
</div>
<div id="peopleInChurchMap" style="width:800px; height:600px"></div>


